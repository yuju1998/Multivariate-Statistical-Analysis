data survey;
infile"F:\大三\多變量\mydata\DATA.csv" dsd firstobs=2;
input x1-x10 y1 y2;
proc print data=survey;
run;

ods pdf  file="C:\Users\lab-107\Desktop\DATAmanova.pdf";
proc glm data=survey;
